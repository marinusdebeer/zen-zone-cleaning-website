{
  "name": "zen-zone-website",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "prebuild": "node scripts/optimizeImages.js",
    "postbuild": "node scripts/versionAssets.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "purge:cf": "node scripts/purgeCloudflare.js",
    "deploy:cf": "npm run deploy && npm run purge:cf",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:cov": "react-scripts test --coverage --watchAll=false",
    "format": "prettier --write .",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react-snap": "^1.23.0",
    "sharp": "^0.33.4"
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "crawl": false,
    "skipThirdPartyRequests": true,
    "include": [
      "/",
      "/house-cleaning-services-barrie",
      "/house-cleaning-services-orillia",
      "/house-cleaning-services-innisfil",
      "/book",
      "/blog",
      "/blog/how-to-prepare-your-home-for-a-professional-cleaning",
      "/blog/recurring-vs-one-time-cleaning-which-is-right-for-you",
      "/blog/the-ultimate-post-renovation-cleaning-checklist",
      "/blog/how-to-keep-your-bathroom-sparkling-between-cleans",
      "/blog/a-guide-to-eco-friendly-cleaning-products",
      "/blog/moving-cleaning-checklist-move-in-move-out"
    ]
  }
}
